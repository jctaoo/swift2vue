{
  "name": "swift2vue",
  "version": "1.0.0",
  "description": "Convert SwiftUI To Vue3",
  "main": "index.js",
  "files": [
    "index.d.ts",
    "index.js"
  ],
  "napi": {
    "name": "swift2vue",
    "triples": {
      "defaults": false,
      "additional": [
        "aarch64-apple-darwin",
        "x86_64-pc-windows-gnu"
      ]
    }
  },
  "engines": {
    "node": ">= 10"
  },
  "scripts": {
    "artifacts": "napi artifacts",
    "build": "napi build --platform --release --pipe \"prettier -w\"",
    "build:debug": "napi build --platform --pipe \"prettier -w\"",
    "format:prettier": "prettier . -w",
    "napi": "napi"
  },
  "devDependencies": {
    "@napi-rs/cli": "^2.16.4",
    "@swc-node/register": "^1.6.8",
    "@swc/core": "^1.3.95",
    "chalk": "^5.3.0",
    "npm-run-all2": "^6.0.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "prettier": {
    "printWidth": 120,
    "semi": false,
    "trailingComma": "all",
    "singleQuote": true,
    "arrowParens": "always"
  }
}